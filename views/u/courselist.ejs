<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Course list</title>
  </head>
  <body>
      <a href="/u/addcourse">Add a new course</a><br>
    <table border='1' style="width:100%">
      <tr>
        <th>id</th>
        <th>name</th>
        <th>start</th>
        <th>end</th>
        <th>Link to update</th>
      </tr>
    <% for (var i=0; i<courses.length; i++) { %>
      <tr>
        <td><%= courses[i].id %></td>
        <td><%= courses[i].name %></td>
        <td><%= courses[i].start %></td>
        <td><%= courses[i].end %></td>
        <td><a href="/u/updatecourse?id=<%= courses[i].id%>">Update Course</a></td>
      </tr>
    <% } %>
    <table>
  </body>
</html>
